using System;
using cmsx.object;

int main(int argc, const char** argv)
{
    try
    {
        for (int i = 1; i < argc; ++i)
        {
            string arg = argv[i];
            UniquePtr<File> file = ReadFile(arg);
            file->AddSymbolsToAddressMap();
            file->Dump(Console.Out());
        }
    }
    catch (const Exception& ex)
    {
        Console.Error() << ex.Message() << endl();
        return 1;
    }
    return 0;
}
